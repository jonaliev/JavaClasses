package com.class8;

public class ForLoop {

	public static void main(String[] args) {
		
		//say good morning 5 times 
		// initialize; test condition; increment
		for (int i=0;   i<=4;    i++) {
		     System.out.println("Good morning"); }
		
		for (int i=1; i<=100; i++){
			System.out.print(i+" ");
	
		}
		
		for (int i=100; i>=1; i--) {
			System.out.println(i);
			
			
		}
		    
		for (int i=1; i<=20; i++) {
			if
			(i%2==0)
				System.out.println(i); 
		}
		
		for(int i=20; i<=50; i++) {
			
			if(i%2==1)
				System.out.println(i);
			
		}
		
		

	} 
	
	
	
	
			
	
	   
          


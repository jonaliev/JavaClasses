package com.class12;

public class StringManipulationDemo3 {

	public static void main(String[] args) {
		

		
		
		
		
		
		String str3="Hello";
        String str4="World";
        String str5="People";
        
        System.out.println(str4+" "+str3+str5);
        
        System.out.println(str3+" ".concat(" "+str4).concat(" "+str5));
	}

}

package com.class10;

public class Task1 {
	public static void main(String[] args) {
		
		char[] grades=new char[6];
		grades[0]='A';
		grades[1]='B';
		grades[2]='C';
		grades[3]='D';
		grades[4]='E';
		grades[5]='F';
		
		System.out.println(grades[1]);
		
		//way 2
		
		char []grades={'A','B','C','D','E','F'};
		System.out.println(grades[1]);
		
		
	}
	

}

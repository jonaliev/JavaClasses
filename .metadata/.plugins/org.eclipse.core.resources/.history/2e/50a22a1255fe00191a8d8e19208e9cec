package com.class14;

public class StringMethodsRecap {

	public static void main(String[] args) {
		
		
		//.replace 
		
		String str="your Syntax Tecjnologies";
		System.out.println(str.replace('y','i'));
		
		
		System.out.println("==================");
		System.out.println(str.replace("your","my"));
		
	}

}
